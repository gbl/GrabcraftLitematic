{
    "schemaVersion": 1,
    "id": "grabcraft-downloader",
    "version": "${version}",
    "environment": "client",
    "entrypoints": {
        "client": [ "de.guntram.mcmod.GrabcraftLitematic.FabricMod" ]
    },
    "custom": {
        "modupdater": {
            "strategy": "json",
            "url": "https://raw.githubusercontent.com/gbl/ModVersionInfo/master/GrabcraftLitematic.json"
        }
    },
    "mixins": [
    ],
    "depends": {
      "fabric": "*"
    },
    "recommends": {
      "modmenu" : "*",
      "modupdater": ">=1.1.7"
    },
    "name": "GrabcraftLitematic",
    "description": "Download schemas from GrabCraft in Litematica format",
    "icon": "icon.png",
    "authors": [ "Giselbaer" ],
    "contact": {
        "homepage": "https://www.curseforge.com/minecraft/mc-mods/grabcraftlitematic",
        "sources": "https://github.com/gbl/GrabcraftLitematic",
        "issues": "https://github.com/gbl/GrabcraftLitematic/issues"
    }
}
